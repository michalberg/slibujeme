<tr>
    <td>
        <%= election.title %> (<%= l election.from %> &ndash; <%= l election.to %>)
    </td>
    <td>
        <div class="btn-group">
            <%= link_to '<i class="icon-edit"></i>'.html_safe, edit_admin_election_path(election), :class => "btn btn-small" %>
            <%= link_to '<i class="icon-trash icon-white"></i>'.html_safe, admin_election_path(election), :method => :delete, :class => "btn btn-small btn-danger", :confirm => "Opravdu chcete volby \"#{election.title}\" smazat?" %>
        </div>
    </td>
</tr>
