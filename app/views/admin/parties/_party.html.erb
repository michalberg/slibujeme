<tr>
    <td>
        <%= party.title %>
    </td>
    <td>
        <div class="btn-group">
            <%= link_to '<i class="icon-edit"></i>'.html_safe, edit_admin_party_path(party), :class => "btn btn-small" %>
            <%= link_to '<i class="icon-trash icon-white"></i>'.html_safe, admin_party_path(party), :method => :delete, :class => "btn btn-small btn-danger", :confirm => "Opravdu chcete stranu \"#{party.title}\" smazat?" %>
        </div>
    </td>
</tr>
